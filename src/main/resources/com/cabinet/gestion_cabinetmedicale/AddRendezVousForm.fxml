      <ComboBox fx:id="minutes" layoutX="222.0" layoutY="279.0" prefHeight="35.0" prefWidth="133.5" promptText="Minutes" style="-fx-background-color: white;" />
      <ComboBox fx:id="stateField" layoutX="88.0" layoutY="348.0" prefHeight="35.0" prefWidth="267.0" promptText="State" style="-fx-background-color: white;" />
    </children>
