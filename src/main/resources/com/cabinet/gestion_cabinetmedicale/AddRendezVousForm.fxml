<AnchorPane prefHeight="465.0" prefWidth="443.0" style="-fx-background-color: #DCDCDC; -fx-border-radius: 20;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.cabinet.gestion_cabinetmedicale.AddRendezVousFormController">
