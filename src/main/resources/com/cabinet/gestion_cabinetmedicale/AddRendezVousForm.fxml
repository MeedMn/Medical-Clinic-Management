        </ImageView>
        <Label layoutX="138.0" layoutY="97.0" text="Ajouter Rendez-Vous">
            <font>
                <Font name="Yu Gothic Bold" size="16.0" />
            </font>
        </Label>
        <Label layoutX="411.0" layoutY="14.0" onMouseClicked="#CloseBtn" prefHeight="31.0" prefWidth="15.0" text="X">
            <font>
                <Font size="24.0" />
            </font>
            <cursor>
                <Cursor fx:constant="HAND" />
            </cursor>
        </Label>
        <TextField fx:id="cneField" focusTraversable="false" layoutX="87.0" layoutY="141.0" prefHeight="35.0" prefWidth="267.0" promptText="Cne" />
